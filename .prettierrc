{
    "useTabs": false,
    "arrowParens": "always",
    "semi": false,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 300,
    "plugins": [
        "prettier-plugin-svelte",
        "prettier-plugin-tailwindcss"
    ],
    "overrides": [{
        "files": "*.svelte",
        "options": { "parser": "svelte" }
    }]
}
